		track		# originating track, if exists
		chain_id
		direction 	# input or output
		endpoint	# needed for routing to loop devices
		type		# redundant, expressed as object class
		device_id 	# what to append to -i: or -o:
		width		# no. of channels
		format		# -f: argument
		ecs_extra	# post_input or pre_output operators
		mono_to_stereo # track method: conversion if needed
		route		# track method: routing
		rec_route	# track method: routing
		full_path
		playat
		region_start
		region_end
		modifiers
