%%Page: (1) 1
%%BeginPageSetup
/pagesave save def
%%EndPageSetup
iso1dict begin
gsave
llx lly 12 add translate
/v 0 store
/x0 x v get 4.431713 add sx cw mul add store
/y0 y v get bfs th add sub store
x0 y0 moveto
0 T () p n
0 T (inputs---->{device_name}->[chain_id1, chain_id2,... ]) N
0 T (           {file}->$state_c{$n}->{file} }->[chain_id1, chain_id2]) N
0 T () S 8 T (       {cooked}->$n->[chain_ida chain_idb,...      ]) N
(5) # () S 8 T (       {mixed}->$n->[chain_ida chain_idb,...      ]) N
0 T () N
0 T () N
0 T (outputs--->device->{$name}[chain_id1, chain_id2,...]) N
0 T (        |_>file->{ $file }->[chain_id1, chain_id2]) N
(10) # () S 8 T () S 16 T (|->loop_id->[ chain_id ]) N
0 T () N
0 T (intermediate->chain_id->"chain operators and routing, etc.") N
0 T () N
0 T (=cut) N
(15) # (sub write_chains {) N
0 T () S 8 T ($debug2 and print "&write_chains\\n";) N
0 T () N
0 T () S 8 T (# the mixchain is usually '1', so that effects applied to ) N
0 T () S 8 T (# track 1 \(session_name\) affect the mix.) N
(20) # () S 8 T (#) N
0 T () S 8 T (# but when playing back a mix, we want mixchain to be ) N
0 T () S 8 T (# something else) N
0 T () S 8 T () N
0 T () S 8 T (my $mixchain = rec_status\(1\) eq "MON") N
(25) # () S 8 T () S 16 T () S 24 T () S 32 T () S 40 T () S 48 T (? $mixchain_aux) N
0 T () S 8 T () S 16 T () S 24 T () S 32 T () S 40 T () S 48 T (: $mixchain;) N
0 T () N
0 T () S 8 T (### SETTING DEVICES AS INPUTS \(used by i.e. rec_setup\)) N
0 T () N
(30) # () S 8 T (for my $dev \(grep{) N
0 T () S 8 T () S 16 T () S 24 T () S 32 T ($_ ne 'file' and $_ ne 'cooked' and $_ ne 'mixed) N
0 T (') N
0 T () S 8 T () S 16 T () S 24 T (} keys %inputs \){) N
0 T () S 8 T () S 16 T ($debug and print "dev: $dev\\n";) N
0 T () S 8 T () S 16 T (push  @input_chains, ) N
(35) # () S 8 T () S 16 T (join " ", "-a:" . \(join ",", @{ $inputs{$dev} }\),) N
0 T () S 8 T () S 16 T () S 24 T ("-f:" .  $devices{$dev}->{input_format},) N
0 T () S 8 T () S 16 T () S 24 T ("-i:" .  $devices{$dev}->{ecasound_id}, ) N
0 T () S 8 T (}) N
0 T () S 8 T (### SETTING LOOPS AS INPUTS \(used by any @oids wanting processed signals) N
0 T (\)) N
(40) # () N
0 T () S 8 T (for my $n \(@all_chains\){) N
0 T () S 8 T () S 16 T (next unless defined $inputs{cooked}->{$n} and @{ $inputs{cooked}) N
0 T (->{$n} };) N
0 T () S 8 T () S 16 T (push  @input_chains, ) N
0 T () S 8 T () S 16 T (join " ", ) N
(45) # () S 8 T () S 16 T () S 24 T ("-a:" . \(join ",", @{ $inputs{cooked}->{$n} }\),) N
0 T () S 8 T () S 16 T () S 24 T ("-i:loop,$n") N
0 T () S 8 T (}) N
0 T () S 8 T (### SETTING MIXLOOPS AS INPUTS \(used by any @oids wanting mixdown signal) N
0 T (s\)) N
0 T () N
(50) # () S 8 T () S 16 T (if \(defined $inputs{mixed} and @{ $inputs{mixed} }\) {) N
0 T () S 8 T () S 16 T () S 24 T (push  @input_chains, ) N
0 T () S 8 T () S 16 T () S 24 T (join " ", ) N
0 T () S 8 T () S 16 T () S 24 T () S 32 T ("-a:" . \(join ",", @{ $inputs{mixed} }\),) N
